    "prettier-eslint": "^6.4.2",
    "prettier-eslint-cli": "^4.1.1",
    "react-addons-test-utils": "^15.6.0",
    "react-autocomplete-cli": "^0.0.3",
    "react-dom": "16.0.0-alpha.12",
    "react-test-renderer": "16.0.0-alpha.12",
    "rimraf": "^2.4.2",
    "snazzy": "^7.0.0",
    "standard": "^10.0.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.2.2",
    "watch": "^0.16.0"
  }
}
